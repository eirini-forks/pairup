{
    "parser": "babel-eslint",
      "parserOptions": {
        "parserOptions": {
          "ecmaVersion": 6,
          "sourceType": "module"
        },
        "extends": "eslint:standard"
      },
    "env": {
        "browser": true,
        "node": true
    },
    "settings": {
        "ecmascript": 6,
        "jsx": true
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "strict": 0,
        "quotes": 0,
        "no-unused-vars": 0,
        "camelcase": 0,
        "no-underscore-dangle": 0
    }
}
